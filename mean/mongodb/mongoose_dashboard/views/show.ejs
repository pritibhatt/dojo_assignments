<html lang="en">
<head>
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
</head>
<body>
        <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <h2><%= all_mongooses.name %>'s profile</h2>
                        <% if(typeof(errors) != 'undefined' ) { %>
                            <% for (var x in errors) { %>
                             <h3><%= errors[x].message %></h3>
                            <% } %>
                        <% } %>
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><%= all_mongooses.name %></td>
                                    <td><a href="/mongooses/edit/<%= all_mongooses._id %>" class="btn btn-default">Change Name</a></td>
                                </tr>
                            </tbody>
                        </table>
                        <a href="/" class="btn btn-default">Go Back</a>
                    </div>
                </div>
            </div>
</body>
</html>